import random as r
import numpy as np

def check(l, l1):
    if l == l1:
        return True
    else:
        return False


def swap(list, pos1, pos2):
    list[pos1], list[pos2] = list[pos2], list[pos1]
    return list

def sort(list):
    return(list.sort())

def bozo(list, sorted):
    length_of_list = len(list)



    count=0

    while check(list, sorted) == False:
        a = np.random.randint((length_of_list))
        b = np.random.randint((length_of_list))
        while b == a:
            b = np.random.randint((length_of_list - 1))
        swap(list, a, b)
        count += 1
        print(list)



    return (count)


urdad = range(1)
for urmom in urdad:
    uruncle = [1,2,3,4,5]
    r.shuffle(uruncle)
    sorted = [1,2,3,4,5]
    print(bozo(uruncle, sorted))
